Host *
    IdentityFile ~/.ssh/id_rsa

Host github.com
    User git

Host gitlab.com
    User git

Host fuel-fake
    HostName 127.0.0.1
    Port 2222
    User stack
    IdentityFile ~/.ssh/id_rsa
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    LogLevel=quiet

Host 10.20.0.*
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    LogLevel=quiet
    
Host dev
    HostName 192.168.56.100
    User stack
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    LogLevel=quiet
